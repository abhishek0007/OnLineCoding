// Find the Number Occurring Odd Number of Times
#include<bits/stdc++.h>

using namespace std;

int oddTimes(int a[], int n) {
    int k = 0;
    for (int i = 0; i < n; i++) {
        k = k ^ a[i];

    }
    return k;
}

int main() {

    int a[] = {3, 3, 1, 4, 4, 2, 1};
    int Arry_size = sizeof(a) / sizeof(a[1]);

    cout << oddTimes(a, Arry_size);
    return 0;
}
