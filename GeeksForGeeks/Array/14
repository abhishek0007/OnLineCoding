//Leaders in an array

#include<bits/stdc++.h>

#define NA -1
using namespace std;

int main() {

    int a[] = {16, 17, 4, 3, 5, 2};
    int n = sizeof(a) / sizeof(a[0]);

    int k = a[n - 1];
    cout << k << " ";
    for (int i = n - 2; i >= 0; i--) {
        if (a[i] > k) {
            k = a[i];
            cout << k << " ";


        }
    }

    return 0;
}
